	var                                                   #include        
	  a: array [1..1000] of string;                                       
	  n, i, j: int64;
	 
	begin
	  readln(n);
	  for i := 1 to n do
	    readln(a[i]);
	    for i := 1 to n do
      for j := 1 to length(a[i]) do
      if a[i][j] in ['0'..'9'] then
        a[i][j] := chr(ord('A') + strtoint(a[i][j]));
end;



var i:integer;
a:array [1..3] of integer;
begin 
for i:=1 to 3 do begin
a[i]:=i;
writeln("Квадрат",a[i]," = ",a[i]*a[i]);
end;
end.


#include <iostream>
using namespace std;
int main()
{
int a[3] = { 1,2,3};
for (int i = 0 ; i < 3 ; i++){
cout << "Квадрат "<< a[i] << " = " << a[i]*a[i] << endl;
}
return 0;
}


