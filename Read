1.	var                                           #include        
2.	  a: array [1..1000] of string;                                       
3.	  n, i, j: int64;
4.	 
5.	begin
6.	  readln(n);
7.	  for i := 1 to n do
8.	    readln(a[i]);
9.	  for i := 1 to n do
10.	    for j := 1 to length(a[i]) do
11.	      if a[i][j] in ['0'..'9'] then
12.	        a[i][j] := chr(ord('A') + strtoint(a[i][j]));
13.	end;



var i:integer;
a:array [1..3] of integer;
begin 
for i:=1 to 3 do begin
a[i]:=i;
writeln("Квадрат",a[i]," = ",a[i]*a[i]);
end;
end.


#include <iostream>
using namespace std;
int main()
{
int a[3] = { 1,2,3};
for (int i = 0 ; i < 3 ; i++){
cout << "Квадрат "<< a[i] << " = " << a[i]*a[i] << endl;
}
return 0;
}


